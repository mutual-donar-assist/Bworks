{
  "name": "csci-5030",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev && react-scripts start",
    "build": "astro check && astro build && react-scripts build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "react-scripts test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@astrojs/check": "^0.4.1",
    "astro": "^4.1.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "settings": {
      "react": {
        "version": "17.0.2"
      }
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
